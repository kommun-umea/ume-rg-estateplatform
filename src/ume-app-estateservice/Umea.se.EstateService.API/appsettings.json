{
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:8080",
      "https://minasidor.dev.umea.se",
      "https://authtokendev.umea.se",
      "https://estate.dev.umea.se",
      "https://estateservice.dev.umea.se"
    ]
  },
  "KeyVaultUrl": "https://umekvestateplatformdev.vault.azure.net/",
  "TokenService": {
    "Address": "https://authtokendev.umea.se/"
  },
  "AllowedHosts": "*",
  "Api": {
    "Name": "EstateService",
    "Title": "Umea.se.EstateService",
    "Description": "Api med koppling mot fastighetsystem",
    "Version": "v1.0",
    "Keys": {
      "Default": "@KeyVault(estateservice-api-key)"
    }
  },
  "ApplicationInsights": {
    "ConnectionString": "@KeyVault(application-insights-connection-string)"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Umea": "Trace"
    },
    "OpenTelemetry": {
      "LogLevel": {
        "Default": "Warning",
        "Umea.se": "Information"
      }
    }
  },
  "Pythagoras-Api-Key": "@KeyVault(pythagoras-api-key)",
  "Pythagoras-Base-Url": "@KeyVault(pythagoras-api-url)",
  "SearchIndex": {
    "RefreshIntervalHours": 24
  }
}
